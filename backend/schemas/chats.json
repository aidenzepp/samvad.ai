{
        "bsonType": "object",
        "required": [
                "id",
                "file_group",
                "model_name",
                "created_at",
                "created_by"
        ],
        "properties": {
                "id": {"bsonType": "string", "description": "must be a UUID"},
                "file_group": {
                        "bsonType": "array",
                        "items": {
                                "bsonType": "object",
                                "required": ["name", "data"],
                                "properties": {
                                        "name": {"bsonType": "string", "description": "must be a string"},
                                        "data": {"bsonType": "string", "description": "must be a base64 encoded string"}
                                }
                        }
                },
                "model_name": {"bsonType": "string", "description": "must be a string"},
                "created_at": {"bsonType": "date", "description": "must be a DateTime"},
                "created_by": {"bsonType": "string", "description": "must be UUID"}
        }
}
